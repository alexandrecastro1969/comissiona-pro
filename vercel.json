{
    "version": 2,
    "buildCommand": "next build",
    "devCommand": "next dev",
    "framework": "nextjs",
    "installCommand": "npm install",
    "regions": ["gru1"],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ]
  }